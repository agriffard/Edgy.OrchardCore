@inject IOptions<AppSettings> AppSettings
@{
    var app = AppSettings.Value;
}
<!DOCTYPE html class="h-100">
<html>
<head>
    <partial name="Head" model="app"></partial>
</head>
<body class="d-flex flex-column h-100">
    <partial name="Header" model="app"></partial>
    <main role="main" class="flex-shrink-0">
        @RenderBody()
    </main>
    <partial name="Footer" model="app"></partial>
    <partial name="FootScripts" model="app"></partial>
</body>
</html>
<!-- @app.Version -->
